    <p>
    <lb n="1"/><milestone type="pagelike" unit="zone" n="A"/><g type="SpiraIL"/>svasti samasta-pra<choice><orig>s</orig><reg>ś</reg></choice>asti-sahitaṁ śrī-siṁhaṇadēva-varuṣada pramāthi-saṁvatsarada ā<choice><orig>ś</orig><reg>ṣ</reg></choice>āḍha pauvurṇṇami<note source="bib:Fleet1899">read paurṇṇami</note> sōmavā
        <lb break="no" n="2"/>radalu pratāpa-vīra-siṁgaṇadēvarasaru banavase-paṁnitsāsiravan āḷuttire <supplied reason="omitted"><g type="ddanda">.</g></supplied> Alliya kaṁpanaṁ nāgarakha<unclear>ṁ</unclear>
        <lb n="3"/><unclear>ḍa</unclear>v-ep<supplied reason="omitted">p</supplied>attaṟa baḷiya bāḍa Abbalūra vistarav-enteṁdaḍe <g type="ddanda">.</g> giḷi-kuḷadiṁdi pi<note>I don't read pi</note>kāvaḷiīn<note source="bib:Fleet1899">read āvaḷiyin</note> oppuva naṁdanadiṁ viḷāsadiṁ 
        <lb n="4"/><unclear>be</unclear>l<note>I read beḷasida</note>asida gaṁdha-śaḷiī<note source="bib:Fleet1899">read sāliyi for śaḷiyiṁ</note> śivālayadōḷiī<note>read ōḷiyi for ōḷiyiṁ</note> bhāḷanētra-pūjita-g<choice><orig>r</orig><reg>ṛ</reg></choice>ihadiṁ  baḷādhika-kaḷāvida-vīra-samūhadiṁdhi
        <lb break="no" n="5"/><unclear>ḷ</unclear>ā-taḷad oḷag abbalūr ativicitrataraṁ nijam āgal oppuguṁ <g type="ddanda">.</g> Antu ś<unclear>ubha</unclear>-vettabbalūra bīra-gauvuḍana<note source="bib:Fleet1899">read gauḍana</note> guṇa
        <lb n="6"/><unclear>ke</unclear>thanav-enteṁdaḍe <g type="ddanda">.</g>
         </p>
    
    <lg n="1" met="campakamāla" xml:lang="kan-Latn">
        <l n="a">parahitan eṁdu dāna-vi<choice><orig>d</orig><reg>dh</reg></choice>an eṁdu guṇ<unclear>āṁ</unclear> bhudhiyeṁdu līleī<note source="bib:Fleet1899">read līleyiṁ</note> <choice><orig>ka</orig><reg>kha</reg></choice></l>
        <l n="b">rakara-tējanaṁ guru-padāṁbu<lb n="7"/>ja-bhaktanan entu no<choice><orig>p</orig><reg>r</reg></choice>paḍa pu</l> 
        <l n="c">rutara-dhairyyanaṁ sakaḷa-vaṁdijanāśrya kaḷpav<choice><orig>r</orig><reg>ṛ</reg></choice>ikṣanaṁ va</l>
        <l n="d">ra-vibudhōktaraṁ mudado baṁ<lb n="8"/><unclear>ṇ</unclear>ṇisuguṁ<note source="bib:Fleet1899">read baṇṇisuguṁ</note> sale bīra-gauvuḍanaṁ</l>
